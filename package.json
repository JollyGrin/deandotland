  "dependencies": {
    "axios": "^0.21.1",
    "next": "^10.0.3",
    "node-sass": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-responsive-masonry": "^2.1.2"
  }
}
