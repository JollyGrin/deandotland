{
  "name": "deanland",
  "version": "1.0.0",
  "description": "place to flex my blender renders",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export -o docs",
    "deploy": "npm run build && echo dean.land > docs/CNAME && git add -A && git commit -m 'update' && git push",
